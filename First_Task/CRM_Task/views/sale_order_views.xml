<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.ui.view" id="sale_order_add_contact">
        <field name="name">sale.order.add.contact</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='sale_header']//group[@name='partner_details']//field[@name='partner_shipping_id']" position="before">
                <field name="order_contact"/>
            </xpath>
        </field>
    </record>


    <record model="ir.ui.view" id="sale_order_change_name">
        <field name="name">sale.order.change.name</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//tree//field[@name='price_unit']" position="attributes">
                <attribute name="string">List Price</attribute>
            </xpath>
        </field>
    </record>
</odoo>